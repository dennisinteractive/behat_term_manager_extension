{
  "name": "dennisdigital/behat-term-manager-extension",
  "type": "behat-extension",
  "description": "Term Manager extension for Behat",
  "keywords": ["term manager", "web", "test"],
  "homepage": "https://github.com/dennisinteractive/behat_term_manager_extension",
  "license": "MIT",
  "require": {
    "behat/mink": "~1.5",
    "behat/behat": "~3.1.0-rc2",
    "behat/mink-extension": "~2.0",
    "symfony/dependency-injection": "~2.7"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0"
  },
  "autoload": {
    "psr-0": {
      "Behat\\TermManagerExtension": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master-dev"
    }
  }
}
